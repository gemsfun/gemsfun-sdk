  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/solmeme-program.git",
    "directory": "sdk-pump"
  },
  "homepage": "https://github.com/your-username/solmeme-program/tree/main/sdk-pump",
  "bugs": {
